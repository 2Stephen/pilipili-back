<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pilipili.pilipiliback.mapper.VideoMapper">

    <!--根据id查询视频-->
    <select id="getVideoById" resultType="com.pilipili.pilipiliback.entity.Video">
        SELECT * FROM video
        WHERE videoid = #{videoid}
    </select>

</mapper>